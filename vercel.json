{
  "rewrites": [
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "/css/:path*"
    },
    {
      "source": "/order",
      "destination": "/order/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
